@model RamaMed.Models.EndocrinoClinicalSymptom
@{
    Layout = "_EndocrinoLayout";
    ViewData["Title"] = " Clinical Symptoms";
}
@{
    UserData userdata = ViewBag.userData;
}



<div class="card">
    <div class="card-header text-info text-uppercase">
        <div class="m-0 p-0 pt-1 float-left"><h4>Clinical Symptoms</h4></div>

        <div class="m-0 p-0 float-right"><a class="btn btn-outline-warning" style="text-decoration:none;" asp-action="Comorbidities">Skip</a></div>
    </div>
    <form method="post" class="card-body pt-0">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

        <div class="w-100 form-group">
            <div class="border-bottom">
                <div class="row p-3">
                    <div class="col-sm-3 text-center">
                        <span class="font-weight-bold">Name : </span>@userdata.Name
                    </div>

                    <div class="col-sm-3">
                        <span class="font-weight-bold text-center">Doctor : </span>@userdata.Doctor
                    </div>

                    <div class="col-sm-3">
                        <span class="font-weight-bold text-center">UHID : </span>@userdata.UHID
                        <input asp-for="@Model.UHID" value="@userdata.UHID" hidden />
                        <span class="text-danger" asp-validation-for="UHID"></span>
                    </div>

                    <div class="col-sm-3">
                        <span class="font-weight-bold text-center">OPDNO : </span>@userdata.OPDNo
                        <input asp-for="@Model.OPDNO" value="@userdata.OPDNo" hidden />
                        <span class="text-danger" asp-validation-for="OPDNO"></span>
                    </div>
                </div>
            </div>
        </div>



        @{
            if (Model != null)
            {
                <input asp-for="@Model.ID" hidden />
                <input asp-for="@Model.CREATEDATE" hidden />
            }
            else
            {
                <div></div>
            }
        }



        <div class="card-body pt-0">
            <div class="row">
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Weight Gain</label>
                        <input class="form-control col-4 mb-2 inputbox" asp-for="@Model.WEIGHT_GAIN_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="WEIGHT_GAIN_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Galactrorrhoea</label>
                        <input class="form-control col-4 mb-2 inputbox" asp-for="@Model.GALACTRORRHOEA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="GALACTRORRHOEA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Increased Apetite</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.INCREASED_APETITE_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="INCREASED_APETITE_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Abdomen Pain</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.ABDOMEN_PAIN_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="ABDOMEN_PAIN_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Palpitation</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.PALPITATION_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="PALPITATION_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Enlargement of Foot</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.ENLARGEMENT_OF_FOOT_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="ENLARGEMENT_OF_FOOT_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Excessive Swating</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.EXCESSIVE_SWEATING_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="EXCESSIVE_SWEATING_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Loss of Libido</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.LOSS_OF_LIBIDO_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="LOSS_OF_LIBIDO_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Oligomenoorhea</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.OLIGOMENOORHEA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="OLIGOMENOORHEA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Skin Dryness</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.SKIN_DRYNESS_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="SKIN_DRYNESS_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Gynecomastia</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.GYNECOMASTIA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="GYNECOMASTIA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Tremor</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.TREMOR_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="TREMOR_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Diplopia</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.DIPLOPIA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="DIPLOPIA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Diarrhoea</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.DIARRHOEA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="DIARRHOEA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Enlargement of Hand</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.ENLARGEMENT_OF_HAND_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="ENLARGEMENT_OF_HAND_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Dysmeonorrhoea</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.DYSMEONORRHOEA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="DYSMEONORRHOEA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Hirsutism</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.HIRSUTISM_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="HIRSUTISM_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Amennorhoea</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.AMENNORHOEA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="AMENNORHOEA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Weight Loss</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.WEIGHT_LOSS_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="WEIGHT_LOSS_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Headache</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.HEADACHE_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="HEADACHE_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Loss of Apetite</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.LOSS_OF_APETITE_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="LOSS_OF_APETITE_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Seizures</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.SEIZURES_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="SEIZURES_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Constipation</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.CONSTIPATION_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="CONSTIPATION_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Infertility</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.INFERTILITY_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="INFERTILITY_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Fatigue</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.FATIGUE_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="FATIGUE_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Pain Tingling/Over Fingers</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.PAIN_TINGLING_OVER_FINGERS_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="PAIN_TINGLING_OVER_FINGERS_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Polymenorrhea</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.POLYMENORRHEA_DURATION" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="POLYMENORRHEA_DURATION"></span>
                    </div>
                </div>
                <div class="col-md-6 px-5 my-1 form-group">
                    <div class="row border-bottom">
                        <label class="form-label col-8">Others</label>
                        <input class="col-4 form-control mb-2 inputbox" asp-for="@Model.OTHERS" placeholder="Duration" />
                        <span class="text-danger" asp-validation-for="OTHERS"></span>
                    </div>
                </div>
            </div>
        </div>

        <div class="card-footer bg-white border-0 mr-3 py-0">
            <input class="float-right btn btn-outline-warning btn-success text-light py-3" type="submit" asp-action="ClinicalSave" value="Next" />
        </div>
        @*<div class="card-footer border-0 mt-0 pt-0">
                <div class="row">
                    <div class="offset-8 col-3">
                        <input class="float-right btn btn-outline-warning btn-success text-light py-3" type="submit" asp-action="ClinicalSave" value="Next" />
                    </div>
                </div>
            </div>*@
    </form>
</div>


@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}


<script>window.onload = function () {
        var element = document.getElementById("CS");
        element.classList.add("activelink");
    };</script>




